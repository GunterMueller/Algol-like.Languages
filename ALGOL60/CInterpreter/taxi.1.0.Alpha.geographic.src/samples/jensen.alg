BEGIN
	INTEGER L; L:=10;
	REAL ARRAY ARR[1:L];
	INTEGER K; REAL RES;

	COMMENT: JENSENâ€™S DEVICE TEST;
	REAL PROCEDURE SUM(I,N,X); VALUE N; INTEGER I,N; REAL X;
	BEGIN
		REAL Y;
		Y:=0;
		FOR I:=1 UNTIL N DO 
			Y:=Y+X;
		SUM:=Y;
	END SUM;

	! MAIN;
	FOR K:=1 UNTIL L DO ARR[K]:=K;
	RES := SUM(K,L,ARR[K]);
	PRINT (RES,5,7)
END MAIN.
	
